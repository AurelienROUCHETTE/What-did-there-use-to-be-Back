<!DOCTYPE html>

<title>Memory</title>

{% block body %}
    <h1>Memory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ memory.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ memory.title }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ memory.content }}</td>
            </tr>
            <tr>
                <th>Picture_date</th>
                <td>{{ memory.pictureDate ? memory.pictureDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Main_picture</th>
                <td>{{ memory.mainPicture }}</td>
            </tr>
            <tr>
                <th>PublishedAt</th>
                <td>{{ memory.publishedAt ? memory.publishedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ memory.createdAt ? memory.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ memory.updatedAt ? memory.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_memory_index') }}">back to list</a>

    <a href="{{ path('app_memory_edit', {'id': memory.id}) }}">edit</a>

    {{ include('memory/_delete_form.html.twig') }}
{% endblock %}
